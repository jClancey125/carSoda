from flask import Flask
app = Flask(__name__)

@app.route('/hello/<name>')
def hello_name(name):
   return 'Hello %s!' % name

@app.route('/guru/<int:drinksNUM.')
def show_butt(drinksNUM):
	return 'Guru Drank %d drinks' % drinksNUM

@app.route('/jon/<float:baddieNUM>')
def show_bodyCount(baddieNUM):
	return 'Jon's body count is %f' % baddieNUM




if __name__ == '__main__':
   app.run('0.0.0.0', debug = True)
